{
  "name": "nikolajlovenhardt/laravel-keen-io",
  "type": "library",
  "description": "Minimal configuration wrapper for Keen.io in Laravel 5.*",
  "keywords": [
    "laravel",
    "keen.io"
  ],
  "homepage": "https://github.com/nikolajlovenhardt/laravel-keen-io",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj Petersen",
      "email": "nikolaj.lovenhardt@gmail.com",
      "homepage": "http://github.com/nikolajlovenhardt"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "laravel/framework": "5.*",
    "keen-io/keen-io": "~2.5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "codeclimate/php-test-reporter": "~0.3.0",
    "phpmd/phpmd" : "~2.4.1"
  },
  "autoload": {
    "psr-4": {
      "LaravelKeenIO\\": "src/LaravelKeenIO"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelKeenIOTests\\": "tests/"
    }
  }
}